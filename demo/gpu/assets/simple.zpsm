.out o0 position 
.out o1.xy texture_coordinate_0.xy

.alias iPos v0
.alias iUv v1

.alias oPos o0
.alias oUv o1

; layout(location = 0) uniform mat4x4 uPersp;
.alias uPerspM0 f0
.alias uPerspM1 f1
.alias uPerspM2 f2
.alias uPerspM3 f3

.entry main vertex

main:
    ; oPos = uPersp * iPos;
    dp4 oPos.x, uPerspM0, iPos 
    dp4 oPos.y, uPerspM1, iPos 
    dp4 oPos.z, uPerspM2, iPos 
    dp4 oPos.w, uPerspM3, iPos 

    ; oUv = iUv;
    mov oUv, iUv.xy
    end
